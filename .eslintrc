{
    "extends": ["react-app", "react-app/jest", "plugin:prettier/recommended", "prettier/react"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": ["error", { "singleQuote": true, "endOfLine":"auto" }]
    },
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint"
}